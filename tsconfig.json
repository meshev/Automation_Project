{
  "compilerOptions": {
    "resolveJsonModule": true,
    "target": "ES2019",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "./dist",
  },
  "include": [
    "src/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "allure-report",
    "allure-results"
  ]
}
